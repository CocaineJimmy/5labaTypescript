{
    "compilerOptions": {
      "target": "ES2019",
      "module": "commonjs",
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "strictNullChecks": true
    },
    "include": ["src/**/*"]
  }
  